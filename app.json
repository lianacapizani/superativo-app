{
  "expo": {
    "name": "superativo-app",
    "slug": "superativo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "superativoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
     "extra": {
      "FIREBASE_API_KEY": "AIzaSyCZEiYg7N8_MniHVrKjg_zAYnd2PsEhaVs",
      "FIREBASE_AUTH_DOMAIN": "superativoapp.firebaseapp.com",
      "FIREBASE_PROJECT_ID": "superativoapp",
      "FIREBASE_STORAGE_BUCKET": "superativoapp.firebasestorage.app",
      "FIREBASE_MESSAGING_SENDER_ID": "320447428218",
      "FIREBASE_APP_ID": "1:320447428218:web:9da3e70ba5a74e16b52dc3"
    },
     "splash": {
      "image": "./assets/splash.png", 
      "resizeMode": "contain",        
      "backgroundColor": "#016DAD"     
    }
  }
}
